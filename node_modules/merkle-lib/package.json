  },
  "license": "MIT",
  "author": "Daniel Cousens",
  "files": [
    "index.js",
    "fastRoot.js",
    "proof.js"
  ],
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bitcoinjs/merkle-lib.git"
