{
  "name": "acorn",
  "description": "ECMAScript parser",
  "homepage": "https://github.com/ternjs/acorn",
  "main": "dist/acorn.js",
  "jsnext:main": "dist/acorn.es.js",
  "version": "3.3.0",
  "engines": {
    "node": ">=0.4.0"
  },
  "maintainers": [
    {
      "name": "Marijn Haverbeke",
      "email": "marijnh@gmail.com",
      "web": "http://marijnhaverbeke.nl"
    },
    {
      "name": "Ingvar Stepanyan",
